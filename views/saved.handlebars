{{#each saved}}
	<div data-_id=5d095edf1100920015e53636 class="card">
      <div class="card-header">
        <h3>
          <a class="article-link" target="_blank" rel="noopener noreferrer" href="https://www.nytimes.com/{{this.link}}">{{this.title}}</a>
          <a class="btn btn-danger delete">Delete From Saved</a>
          <a class="btn btn-info notes">Article Notes</a>
        </h3>
      </div>
      <div class="card-body">{{this.summary}}</div>
    </div>
	
{{/each}}